{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
      "build": {
        "dependsOn": ["^build"],
        "inputs": ["$TURBO_DEFAULT$", ".env*"],
        "outputs": [".svelte-kit/**", ".vercel/**", ".netlify/**", "dist/**"]
      },
      "lint": {
        "dependsOn": ["^lint"]
      },
      "check": {
        "dependsOn": ["^check"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "generate": {
        "dependsOn": ["^generate"]
      }
    }
  }
  